type Nat {
    ctor zero;
    ctor s(Nat);
}

pred even(Nat) {
    even(zero) <- true;
    even(s(s(let x))) <- even(x);
}

pred add(Nat, Nat, Nat) {
    add(let x, zero, x) <- true;
    add(let x, s(let y), s(let z)) <- add(x, y, z);
}

pred main {
    main <- add(s(s(zero)), s(s(zero)), s(s(s(s(zero)))));
}
