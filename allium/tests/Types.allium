type Void {}

type Unit {
    ctor Unit;
}

type Bool {
    ctor T;
    ctor F;
}

type IceCreamFlavor {
    ctor Chocolate;
    ctor Vanilla;
    ctor Strawberry;
}

type Sauce {
    ctor Fudge;
    ctor Caramel;
    ctor Raspberry;
}

type Sundae {
    ctor Sundae(IceCreamFlavor, Sauce);
}

pred hasVanilla(Sundae) {
    hasVanilla(Sundae(Vanilla, _)) <- true;
}

pred main {
    main <- hasVanilla(Sundae(Vanilla, Raspberry));
}

// CHECK: prove: main()
