pred saysHello(String) {
    saysHello("Hello world!") <- true;
}

pred main {
    main <-
        saysHello(let x),
        saysHello(x),
        saysHello("Hello world!");
}

// CHECK: prove: main()
// CHECK: prove: saysHello(var 0, )
// CHECK: prove: saysHello(var 0, )
// CHECK: prove: saysHello(Hello world!, )
// CHECK: Exit code: 0
